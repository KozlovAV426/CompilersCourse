class FirstClass {
    public static void main() {
        int z;
        int q;
        z = 1;
        q = 2;
        System.out.println(z);
        {
            int z;
            z = 13;
            System.out.println(z);
        }
        z = q;
        System.out.println(z);
        if (z == q) {z = 10;} else {z = q - 1;}
    }
}

class SClass {
    public int A() {
        int z;
        int q;
        int[] a;
        a = new int[10];
    }
}

class TClass {
    public int B() {
        SClass tmp;
        int y;
        y = tmp.A();
    }
}



